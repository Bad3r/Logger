{
  "name": "Logseq",
  "productName": "Logseq",
  "version": "0.9.9",
  "main": "electron.js",
  "author": "Logseq",
  "license": "AGPL-3.0",
  "description": "A privacy-first, open-source platform for knowledge management and collaboration.",
  "repository": "https://github.com/logseq/logseq",
  "scripts": {
    "electron:dev": "electron-forge start",
    "electron:debug": "electron-forge start --inspect-electron",
    "electron:make": "electron-forge make",
    "electron:make-macos-arm64": "electron-forge make --platform=darwin --arch=arm64",
    "electron:publish:github": "electron-forge publish",
    "rebuild:all": "electron-rebuild -v 25.2 -f",
    "postinstall": "install-app-deps"
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "@fastify/cors": "8.2.0",
    "@logseq/rsapi": "0.0.70",
    "@sentry/electron": "2.5.1",
    "abort-controller": "3.0.0",
    "better-sqlite3": "8.0.1",
    "chokidar": "^3.5.1",
    "command-exists": "1.2.9",
    "diff-match-patch": "1.0.5",
    "dugite": "2.5.0",
    "electron-deeplink": "1.0.10",
    "electron-dl": "3.5.0",
    "electron-log": "4.4.8",
    "electron-squirrel-startup": "1.0.0",
    "electron-window-state": "5.0.3",
    "extract-zip": "2.0.1",
    "fastify": "^4.18.0",
    "fs-extra": "9.1.0",
    "https-proxy-agent": "5.0.0",
    "node-fetch": "3.3.1",
    "open": "7.3.1",
    "posthog-js": "1.10.2",
    "semver": "7.3.5",
    "update-electron-app": "2.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron/rebuild": "3.2.13",
    "electron": "25.2.0",
    "electron-builder": "^24.4.0",
    "electron-forge-maker-appimage": "https://github.com/logseq/electron-forge-maker-appimage.git"
  },
  "resolutions": {
    "**/electron": "25.2.0",
    "**/node-gyp": "9.4.0"
  }
}
